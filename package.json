{
  "name": "miscord",
  "version": "3.7.7",
  "description": "Facebook Messenger to Discord bridge",
  "main": "index.js",
  "scripts": {
    "start": "node bin/index.js",
    "postinstall": "node bin/postinstall.js",
    "test": "standard",
    "build": "pkg --public .",
    "build:win": "pkg -t node8-win-x64 --public .",
    "build:win32": "pkg -t node8-win-x86 --public .",
    "build:mac": "pkg -t node8-mac-x64 --public .",
    "build:linux": "pkg -t node8-linux-x64 --public .",
    "build:linux32": "pkg -t node8-linux-x86 --public ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bjornskjald/miscord/"
  },
  "homepage": "https://miscord.net",
  "keywords": [
    "messenger",
    "discord",
    "javascript",
    "discord.js",
    "bridge",
    "bot"
  ],
  "bin": {
    "miscord": "bin/index.js"
  },
  "author": "Bjornskjald",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.3.0",
    "discord.js": "^11.3.2",
    "dot-prop": "^4.2.0",
    "emoji-strip": "^1.0.1",
    "facebook-chat-api": "1.6.0",
    "js-yaml": "^3.12.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "npmlog": "^4.1.2",
    "remove-accents": "^0.4.2",
    "request": "^2.87.0",
    "semver": "^5.5.0",
    "strip-ansi": "^4.0.0",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "pkg": "^4.3.3",
    "standard": "^11.0.1"
  },
  "pkg": {
    "scripts": [
      "node_modules/facebook-chat-api/**/*.js",
      "!node_modules/facebook-chat-api/**/test"
    ]
  },
  "standard": {
    "globals": [
      "config"
    ]
  }
}
